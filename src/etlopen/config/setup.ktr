    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var TSTAMP;

var v = getVariable(&quot;RUNDATE&quot;, &quot;x&quot;);
println(&quot;======================================== using RUNDATE=&quot;+v);


if ( &quot;x&quot;!= v ) {
	&#47;&#47; parse date
	TSTAMP = str2date(v,&quot;dd&#47;MM&#47;yyyy&quot;);
} else {
	var d = new Date();

	d.setHours(d.getHours() - 12 );
	d.setHours(0);
	d.setMinutes(0);
	d.setSeconds(0);
	TSTAMP = d;
}


</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>TSTAMP</name>
