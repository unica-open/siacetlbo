    <sql>select a.codice_istituto||a.codice_ente||a.anno_esercizio||a.indicativo_entrate_uscite||a.codifica_bilancio||a.numero_articolo||a.anno_residuo||a.descr_codifica_bilancio_pt1||a.descr_codifica_bilancio_pt2||a.colonna_1||a.colonna_2||a.codice_meccanografico||a.colonna_3||a.importo_capitolo||a.importo_cassa||a.colonna_4||a.colonna_5||a.colonna_6||a.importo_impegnato||a.importo_fondo_vincolato||a.colonna_7||a.colonna_8||a.colonna_9||a.colonna_10||a.colonna_11||a.colonna_12||a.colonna_13||a.colonna_14||a.colonna_15||a.colonna_16 riga_tracciato 
from  bilarm_tracciato a
where 0=0
and   a.elab_id = (select max(elab_id)
                   from bilarm_tracciato b
                   where a.ente_proprietario_id = b.ente_proprietario_id
                   and   a.anno_esercizio = b.anno_esercizio
                  )
and   a.ente_proprietario_id = ${ente_proprietario_id}
and   a.anno_esercizio = &apos;${anno_bilancio}&apos;

</sql>
