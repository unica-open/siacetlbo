    <sql>select
cast(&apos;${CLASSIF_FAM_TREE_ID}&apos; as int) cc_classif_fam_tree_id, 
cast(&apos;${ENTE}&apos; as int) cc_ente_proprietario_id,
to_timestamp(&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;MM&#47;yyyy&apos;)  AT TIME ZONE &apos;UCT&apos;,
&apos;${LOGIN_OPERAZIONE}&apos; cc_login_operazione,
tbo.cc_classif_code code_figlio,
tbo.cc_nrrecord_letto record_letto_figlio,
tbo.cc_classif_id id_figlio, tbo.cc_livello livello_figlio, tbo.cc_ordine ordine_figlio,
tb.cc_classif_code code_padre,
tb.cc_nrrecord_letto record_letto_padre,
tb.cc_classif_id id_padre,tb.cc_livello livello_padre
from siac.SIAC_W_CARICAMENTO_CLASS tbo, siac.SIAC_W_CARICAMENTO_CLASS tb
where 
tbo.cc_classif_fam_tree_id = ${CLASSIF_FAM_TREE_ID} 
and tb.cc_classif_fam_tree_id=tbo.cc_classif_fam_tree_id
and tb.cc_livello = tbo.cc_livello-1
and case 
	when tbo.cc_livello = 5 THEN	
		SUBSTRING(tbo.cc_classif_code,1,13) = SUBSTRING(tb.cc_classif_code,1,13)
    when tbo.cc_livello = 4 THEN
    	SUBSTRING(tbo.cc_classif_code,1,9) = SUBSTRING(tb.cc_classif_code,1,9)
	when tbo.cc_livello = 3 THEN		
		SUBSTRING(tbo.cc_classif_code,1,6) = SUBSTRING(tb.cc_classif_code,1,6)
	when tbo.cc_livello = 2 THEN		
		SUBSTRING(tbo.cc_classif_code,1,3) = SUBSTRING(tb.cc_classif_code,1,3)
    else
    1=1
    END
and tbo.cc_livello &gt; 1
union
select ${CLASSIF_FAM_TREE_ID} cc_classif_fam_tree_id, 
${ENTE} cc_ente_proprietario_id,
to_timestamp(&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;MM&#47;yyyy&apos;)  AT TIME ZONE &apos;UCT&apos;,
&apos;${LOGIN_OPERAZIONE}&apos; cc_login_operazione,
tbo.cc_classif_code code_figlio,
tbo.cc_nrrecord_letto record_letto_figlio,
tbo.cc_classif_id id_figlio, tbo.cc_livello livello_figlio, tbo.cc_ordine ordine_figlio,
null code_padre,
null record_letto_padre,
null id_padre,null livello_padre
from siac.SIAC_W_CARICAMENTO_CLASS tbo
where 
tbo.cc_classif_fam_tree_id = ${CLASSIF_FAM_TREE_ID} and 
tbo.cc_livello = 1;</sql>
