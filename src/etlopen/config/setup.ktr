    <connection>siacdb</connection>
    <sql>select a.extsys_ente_code codice_ente, b.ente_proprietario_id
from siac.siac_r_sistema_esterno_ente a, siac.siac_d_sistema_esterno b
where a.extsys_id=b.extsys_id
and b.extsys_code=&apos;ELAB_BATCH&apos; 
and b.data_cancellazione is null
and a.data_cancellazione is null
and now() between a.validita_inizio and coalesce(a.validita_fine,now())
and extsys_ente_code = &apos;${codice_ente}&apos;;</sql>
