        <jsScript_script>&#47;&#47; costruisce il campo con l&apos;espressione regolare da usare per il match dei file (EMAT,EMAP,EMFE)

&#47;&#47; cod_oil = getVariable(&quot;oil.ente_codice&quot;,&quot;xxxxxx&quot;);
&#47;&#47;var f_regexp = &quot;^&quot; + getVariable(&quot;data_elaborazione&quot;,&quot;UNDEFINED&quot;)+&quot;[0-9]+&quot;+&quot;_&quot;+ente_oil_codice_opi+&quot;_&quot;+flusso_elab_mif_nome_file + &quot;_[0-9]+\\.[A-Za-z]+&quot;;
&#47;&#47;Es. Nome file : RPI_OPI_2017-11-14_005500_2017-11-14_GdC_01
&#47;&#47;var f_regexp = &quot;^&quot; + ente_oil_codice_opi +&quot;_(20)\\d\\d[-&#47;.](0[1-9]|1[012])[- &#47;.](0[1-9]|[12][0-9]|3[01])&quot;+&quot;_[0-9]+
var f_regexp =  &quot;^.+_&quot;+getVariable(&quot;data_elaborazione&quot;,&quot;UNDEFINED&quot;)+&quot;_&quot;+flusso_elab_mif_nome_file + &quot;_[0-9]+\\.[A-Za-z]+$&quot;;
&#47;&#47;+&quot;[0-9]+&quot;+&quot;\\-&quot;+&quot;[0-9]+&quot;+&quot;\\-&quot;+&quot;[0-9]+&quot;
var f_folder = getVariable(&quot;oil.mandati.fromla&quot;,&quot;UNDEFINED&quot;)+&quot;&#47;&quot;+getVariable(&quot;data_elaborazione&quot;,&quot;UNDEFINED&quot;);
</jsScript_script>
