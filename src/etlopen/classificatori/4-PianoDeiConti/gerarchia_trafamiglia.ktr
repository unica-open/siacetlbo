    <replace_variables>Y</replace_variables>
    <sql>insert into siac.siac_r_class
(classif_a_id,classif_b_id,validita_inizio,ente_proprietario_id,login_operazione)
select 
c.classif_id, w.cc_classif_id, to_timestamp(&apos;?&apos;,&apos;dd&#47;mm&#47;yyyy&apos; ),&apos;?&apos;,&apos;?&apos;
from siac.siac_w_caricamento_class w, siac.siac_t_class_fam_tree fam, siac.siac_t_class c, siac.siac_r_class_fam_tree r
where 
w.cc_classif_fam_tree_id = ? and w.cc_ente_proprietario_id = ? and w.cc_classif_b_code is not null and w.cc_EU = &apos;E&apos;
and
fam.ente_proprietario_id = ? and fam.class_fam_code = &apos;Entrata - TitoliTipologieCategorie&apos; and to_timestamp(&apos;?&apos;,&apos;dd&#47;mm&#47;yyyy&apos; ) between fam.validita_inizio AND coalesce (fam.validita_fine, to_timestamp(&apos;?&apos;,&apos;dd&#47;mm&#47;yyyy&apos; ))
and r.classif_fam_tree_id = fam.classif_fam_tree_id
and r.classif_id = c.classif_id
and c.classif_code = w.cc_classif_b_code;
</sql>
