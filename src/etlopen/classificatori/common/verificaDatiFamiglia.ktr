    <connection>Siac</connection>
    <sql>    select count(*) as countFam
from siac.siac_t_class_fam_tree tfam
JOIN siac.siac_d_class_fam dfam on 
	(dfam.classif_fam_id=tfam.classif_fam_id
	and dfam.classif_fam_desc=&apos;${CLASSIFICATORE}&apos;
    and dfam.ente_proprietario_id=${ENTE}
	and now() between dfam.validita_inizio and coalesce(dfam.validita_fine,now())
	)
where 
	--to_timestamp (&apos;${VALIDITA_FINE_PREGRESSO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;) between tfam.validita_inizio 
	--and coalesce (tfam.validita_fine, to_timestamp (&apos;${VALIDITA_FINE_PREGRESSO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;))
	to_timestamp (&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;) - interval &apos;24 hours&apos; between tfam.validita_inizio 
	and coalesce (tfam.validita_fine, to_timestamp (&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;)- interval &apos;24 hours&apos;)
    and tfam.data_cancellazione is null</sql>
