    <connection>Siac</connection>
    <sql>-- controllo non esista una famiglia dello stesso tipo, per lo stesso ente, valida nel range di validità della nuova famiglia di classificatori
select
*
from siac.siac_t_class_fam_tree
JOIN siac.siac_d_class_fam on 
	(siac_d_class_fam.classif_fam_id=siac_t_class_fam_tree.classif_fam_id
	and siac_d_class_fam.classif_fam_desc=&apos;${CLASSIFICATORE}&apos;
    and siac_d_class_fam.ente_proprietario_id=${ENTE}
	and now() between siac_d_class_fam.validita_inizio and coalesce(siac_d_class_fam.validita_fine,now())
	)
where 
-- inizio validita nuova famiglia 
 (to_timestamp (&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;) between siac_t_class_fam_tree.validita_inizio 
--and coalesce (siac_t_class_fam_tree.validita_fine, to_timestamp (&apos;${VALIDITA_INIZIO}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;))
and siac_t_class_fam_tree.validita_fine)
-- fine validita nuova famiglia
-- or  to_timestamp (&apos;${VALIDITA_FINE}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;) between siac_t_class_fam_tree.validita_inizio and coalesce (siac_t_class_fam_tree.validita_fine, to_timestamp (&apos;${VALIDITA_FINE}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;)))
-- and siac_t_class_fam_tree.data_cancellazione is null;</sql>
